{
  "name": "crypto-pair",
  "version": "0.3.8",
  "description": "Normalize cryptocurrency trade pairs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist/cli.js",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "prettier": "prettier -c --write '**/*'",
    "prepare": "npm run lint && npm run prettier && npm run test && npm run build",
    "test": "jest --config jest.config.json --no-cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/crypto-pair.git"
  },
  "keywords": [
    "cryptocurrency",
    "blockchain",
    "trade"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "soulmachine",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/soulmachine/crypto-pair/issues"
  },
  "homepage": "https://github.com/soulmachine/crypto-pair",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "exchange-info": "^1.6.5",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-each": "^26.4.2",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "yargs": "^16.0.3"
  }
}
